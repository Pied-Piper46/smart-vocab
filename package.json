{
  "name": "vocab-app",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npx prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "words:add": "npx dotenv -e .env.local -- npx tsx prisma/seed.ts",
    "words:update": "npx dotenv -e .env.local -- npx tsx prisma/seed.ts --force",
    "words:clear": "npx dotenv -e .env.local -- npx tsx prisma/seed.ts --clear",
    "words:verbose": "npx dotenv -e .env.local -- npx tsx prisma/seed.ts --verbose",
    "db:seed": "npx dotenv -e .env.local -- npx tsx prisma/seed.ts"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.1",
    "@prisma/client": "^6.9.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "lucide-react": "^0.514.0",
    "next": "15.3.3",
    "next-auth": "^5.0.0-beta.28",
    "prisma": "^6.9.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "tailwindcss": "^4",
    "tsx": "^4.20.2",
    "typescript": "^5"
  }
}
